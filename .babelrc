{
  "env": {
    "client": {
      "presets": ["react", ["env", {
        "targets": { "browsers": "> 2%" }
      }]]
    },
    "server": {
      "presets": ["react", ["env", {
        "targets": { "node": true }
      }]],
      "plugins": [
        ["transform-object-rest-spread", { "useBuiltIns": true }]
      ]
    }
  }
}
